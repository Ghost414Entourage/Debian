from github import Github
import os

# Load GitHub token from environment
GITHUB_TOKEN = os.getenv("GITHUB_TOKEN")
if not GITHUB_TOKEN:
    raise ValueError("Please set your GITHUB_TOKEN environment variable.")

# Authenticate with GitHub
g = Github(GITHUB_TOKEN)

# Search repositories by keyword
search_keywords = "OpenWrt"
max_results = 40

# GitHub search query (searches in name, description, and readme by default)
query = f"{search_keywords} sort:stars"
repositories = g.search_repositories(query=query, sort="stars", order="desc")

print(f"Top {max_results} repositories for search '{search_keywords}':\n")
for i, repo in enumerate(repositories[:max_results], start=1):
    print(f"{i}. {repo.full_name} - ‚≠ê {repo.stargazers_count}")
    print(f"   URL: {repo.html_url}\n")



